#设置编码
SET NAMES UTF8;
#丢弃dd，如果存在
DROP DATABASE IF EXISTS dd;
#创建数据库加编码模式
CREATE DATABASE dd CHARSET=UTF8;
#进入数据库
USE dd;
#创建数据表
CREATE TABLE user(
  uid INT PRIMARY KEY AUTO_INCREMENT,
  phone varchar(16),
  user_name varchar(32),
  email varchar(30),
  uname varchar(20),
  upwd varchar(20),
  gender smallint
);
